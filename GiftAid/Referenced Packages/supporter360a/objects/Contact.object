    <validationRules>
        <fullName>Preferred_Email_Address_Validation</fullName>
        <active>true</active>
        <description>Checks that the preferred email address that has been chosen exists on the record</description>
        <errorConditionFormula>OR
(	AND
	(	TEXT(00Nb00000064tE4) = &apos;Personal&apos;,
		ISBLANK(00Nb00000064tE2)
	),
	AND
	(	TEXT(00Nb00000064tE4) = &apos;Work&apos;,
		ISBLANK(00Nb00000064tE5)
	),
	AND
	(	TEXT(00Nb00000064tE4) = &apos;Other&apos;,
		ISBLANK(00Nb00000064tE1)
	)
)</errorConditionFormula>
        <errorMessage>Preferred email address doesn&apos;t exist on record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Preferred_Mailing_Address_Validation</fullName>
        <active>true</active>
        <description>Validation rule for &quot;Preferred Mailing Address&quot;</description>
        <errorConditionFormula>IF
(	AND
	(	00Nb00000064tDJ == TRUE,
		00Nb00000064tDY == TRUE
	),
	TRUE,
	FALSE
)</errorConditionFormula>
        <errorMessage>Only one address can be marked as the Preferred Mailing Address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Preferred_Phone_Number_Validation</fullName>
        <active>true</active>
        <description>Checks that the preferred phone number that has been chosen exists on the record</description>
        <errorConditionFormula>OR
(	AND
	(	TEXT(00Nb00000064tF3) = &apos;Home&apos;,
		ISBLANK(00Nb00000064tEz)
	),
	AND
	(	TEXT(00Nb00000064tF3) = &apos;Work&apos;,
		ISBLANK(00Nb00000064tF4)
	),
	AND
	(	TEXT(00Nb00000064tF3) = &apos;Mobile&apos;,
		ISBLANK(00Nb00000064tF0)
	),
	AND
	(	TEXT(00Nb00000064tF3) = &apos;Other&apos;,
		ISBLANK(00Nb00000064tF1)
	),
	AND
	(	TEXT(00Nb00000064tF3) = &apos;Fax&apos;,
		ISBLANK(00Nb00000064tEy)
	)
)</errorConditionFormula>
        <errorMessage>Preferred phone number doesn&apos;t exist on record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Preferred_Street_Address_Validation</fullName>
        <active>true</active>
        <description>Validation rule for &quot;Preferred Street Address&quot;</description>
        <errorConditionFormula>IF
(	AND
	(	00Nb00000064tDK == TRUE,
		00Nb00000064tDZ == TRUE
	),
	TRUE,
	FALSE
)</errorConditionFormula>
        <errorMessage>Only one address can be marked as the Preferred Street Address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Address_Validation</fullName>
        <active>false</active>
        <description>Deprecated</description>
        <errorConditionFormula>FALSE</errorConditionFormula>
        <errorMessage>Deprecated</errorMessage>
    </validationRules>
    <validationRules>
