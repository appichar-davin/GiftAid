        <errorConditionFormula>FALSE</errorConditionFormula>
        <errorMessage>Deprecated</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>MergeRecords</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Merge Records</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var ids = {!GETRECORDIDS($ObjectType.Contact)};
if (ids.length != 2)
{	alert(&apos;Please check exactly two records before clicking the Merge Records button.&apos;);
}
else
{	
var aa = &apos;{!$Api.Enterprise_Server_URL_190}&apos;; 
var split1 = aa.split(&apos;//&apos;); 
var split2 = split1[1].split(&apos;.&apos;); 
window.open(&apos;https://c.&apos;+split2[0]+&apos;.visual.force.com/apex/s360a__RecordMergeManagement?object=Contact&amp;ids=&apos; + ids.join(&apos;,&apos;));
}</url>
    </webLinks>
